<?xml version="1.0" encoding="UTF-8"?>
<!-- HT5XX XML Provisioning Configuration -->
<gs_provision version="1">
<mac><%= mac %></mac>
	<config version="1">
    <!--
      ##############################################################################
      ##  Basic Settings.                                                         ##
      ##############################################################################
    -->
    <!-- End User Password -->
    <P196>B@lpha9001</P196>
    <!-- Web Port -->
    <P901>80</P901>
    <!-- Telnet Server. 1 - No, 0 - Yes -->
    <P276>0</P276>
    <!-- DHCP Hostname -->
    <P146>HT502</P146>
    <!-- DHCP Vendor Class ID -->
    <P148>HT502</P148>
    <!-- IP Address. 0 - DHCP, 2 - PPPoE, 1 - Static IP  -->
    <P8>0</P8>
    <!-- IP Address -->
    <P9>192</P9>
    <P10>168</P10>
    <P11>0</P11>
    <P12>160</P12>
    <!-- Subnet mask -->
    <P13>255</P13>
    <P14>255</P14>
    <P15>255</P15>
    <P16>0</P16>
    <!-- Router -->
    <P17>192</P17>
    <P18>168</P18>
    <P19>0</P19>
    <P20>1</P20>
    <!-- DNS 1 -->
    <P21>1</P21>
    <P22>1</P22>
    <P23>1</P23>
    <P24>1</P24>
    <!-- DNS 2 -->
    <P25>1</P25>
    <P26>0</P26>
    <P27>0</P27>
    <P28>1</P28>
    <!-- Time Zone. Offset in minutes to GMT -->
    <P64>BRST+3BRDT+2,M10.3.0,M2.3.0</P64>
    <!-- Self-Defined Time Zone -->
    <246>MTZ+6MDT+5,M3.2.0,M11.1.0</246>
    <!-- Language. 0 - English, 4 - Chinese, 10 - Spanish IVR -->
    <P342>0</P342>

    <!--
      #################################################
      # NAT/DHCP Server Information & Configuration   #
      #################################################
    -->
    <!-- Device Mode. 0 -NAT Router, 1 - Bridge -->
    <P231>1</P231>

    <!-- Enable LAN DHCP. 0 - No, 1 - Yes -->
    <P5001>0</P5001>

    <!--
      ##############################################################################
      ##  Advanced Settings.                                                      ##
      ##############################################################################
    -->
    <!-- Admin password for web interface  -->
    <P2>Admin</P2>
    <!-- Firmware Upgrade and Privisioning. 0 - TFTP Upgrade, 1 - HTTP Upgrade, 2 - HTTPS Upgrade -->
    <P212>1</P212>
    <!-- Firmware Server Path -->
    <P192><%= firmwarePath %></P192>
    <!-- Config Server Path -->
    <P237><%= provisionPath %></P237>
    <!-- Allow DHCP Option 66 to override server. 0 - No, 1 - Yes. Default is Yes -->
    <P145>0</P145>
    <!-- 3CX Auto Provision. 0 - No, 1 - Yes. Default is 1 - Yes -->
    <P1414>0</P1414>
    <!-- 
      Use firmware pre/postfix to determine if f/w is required
      0 = Always Check for New Firmware 
      1 = Check New Firmware only when F/W pre/suffix changes 
      2 = Always Skip the Firmware Check
     -->
    <P238>0</P238>
    <!-- Automatic Reboot. 0 - No, 1 - Yes, daily at hour, 2 - Yes, weekly on day -->
    <P21929>1</P21929>
    <!-- Hour of the Day(0-23) -->
    <P21930>2</P21930>

    <!-- 
      ##############################################
      # Call Progress Tones                        #
      ##############################################
     -->
    <!-- Dial Tone -->
		<P4000>f1=425@-13,f2=0@-13,c=0/0;</P4000>

		<!-- Ringback Tone -->
		<P4001>f1=425@-13,f2=0@-13,c=1000/4000;</P4001>

		<!-- Busy Tone -->
		<P4002>f1=425@-13,f2=0@-13,c=250/250;</P4002>

		<!-- Reorder Tone -->
		<P4003>f1=425@-13,f2=0@-24,c=250/250;</P4003>

		<!-- Confirmation Tone -->
		<P4004>f1=350@-11,f2=440@-11,c=100/100-100/100-100/100;</P4004>

		<!-- Call Waiting tone -->
		<P4005>f1=440@-13,c=300/10000;</P4005>

		<!-- Prompt tone -->
    <P4041>f1=350@-17,f2=440@-17,c=0/0;</P4041>
    
    <!-- 
      Phone LED Pattern. Default is 0.
      Number: 0, 1, 2.
      0 - Pattern A (Onhook: OFF; Offhook: ON; VM: Blink) 
      1 - Pattern B (Onhook: ON; Offhook: OFF; VM: Blink if phone onhook) 
      2 - Pattern C (Onhook and Registered: ON; Offhook: Blink; VM: Blink 1sec ON/1sec OFF)
     -->
    <P4049>1</P4049>
    <!-- NTP Server -->
    <P30>br.pool.ntp.org</P30>
    <!-- NTP Update Interval in minutes(5-1440)(Default is set to 60 minutes) -->
    <P5005>60</P5005>

    <!--
      ##############################################################################
      ##  FXS PORT 1 Settings.                                                    ##
      ##############################################################################
    -->
    <% if(alterarUser){ %>
      <!-- Primary SIP Server -->
      <P47><%= dominio1 %></P47>
      <!-- Failover SIP Server -->
      <P967><%= dominio1 %></P967>
      <!-- Outbound Proxy -->
      <P48><%= outboundProxy1 %></P48>
      <!-- Backup Outbound Proxy -->
      <P2333><%= outboundProxy1 %></P2333>
      <!-- SIP User ID -->
      <P35><%= usuario1 %></P35>
      <!-- Authentication ID -->
      <P36><%= usuario1 %></P36>
      <!-- Authentication password -->
      <P34><%= password1 %></P34>
      <!-- Name -->
      <P3><%= usuario1 %></P3>
    <% } %>
    <!-- Account Active. 0 - No, 1 - Yes -->
    <P271>1</P271>
    <!-- Prefer Primary SIP Server. 0 - No, 1 - Yes -->
    <P4567>1</P4567>
    <!-- Prefer Primary Outbound Proxy. 0 - No, 1 - Yes. Default is 0 -->
    <P28096>1</P28096>
    <!-- SIP Transport 0 - UDP, 1 - TCP, 2 - TLS -->
    <P130>0</P130>
    <!-- NAT Traversal (STUN) 0 - No, 2 - No but send keep-alive, 1 - Yes -->
    <P52>2</P52>
    <!-- SIP Registration. 0 - No, 1 - Yes -->
    <P31>1</P31>
    <!-- Unregister On Reboot. 0 - No, 1 - Yes -->
    <P81>1</P81>
    <!-- Outgoing call without Registration. 0 - No, 1 - Yes -->
    <P109>0</P109>
    <!-- Register Expiration (in minutes. default 1 hour, max 45 days) -->
    <P32>1</P32>
    <!-- Reregister before Expiration -->
    <P2330>0</P2330>
    <!-- Registration Retry Wait Time, in seconds. Between 1-3600, default is 30 -->]
    <P138>20</P138>
    <!-- SIP Registration Failure Retry Wait Time upon 403 Forbidden -->
    <P26002>120</P26002>
    <!-- Local SIP port (default 5060) -->
    <P40>5060</P40>
    <!-- Local RTP port (1024-65535, default 5004) -->
    <P39>10000</P39>
    <!-- Use Random Port. 0 - No, 1 - Yes -->
    <P78>1</P78>
    <!-- Hold Target Before Refer. 0 - No, 1 - Yes -->
    <P26003>1</P26003>
    <!-- Refer-To Use Target Contact. 0 - No, 1 - Yes -->
    <P135>0</P135>
    <!--- Transfer on Conference Hangup. 0 - No, 1 - Yes -->
    <P4560>0</P4560>
    <!-- Disable Bellcore Style 3-Way Conference. 0 - No, 1 - Yes -->
    <P4830>0</P4830>
    <!-- Remove OBP from Route Header. 0 - No, 1 - Yes -->
    <P4562>0</P4562>
    <!-- Support SIP Instance ID. 0 - No, 1 - Yes -->
    <P288>1</P288>
    <!-- Validate Incoming SIP Message . 0 - No, 1 - Yes -->
    <P4340>0</P4340>
    <!-- Check SIP UserID for incoming INVITE. 0 - No, 1 - Yes -->
    <P258>1</P258>
    <!-- Authenticate incoming INVITE -->
    <P2346>0</P2346>
    <!-- Authenticate server certificate domain. 0 - No, 1 - Yes -->
    <P2311>0</P2311>
    <!-- Authenticate server certificate chain. 0 - No, 1 - Yes -->
    <P2367>0</P2367>
    <!--
      Trusted CA certificates.
      String
      P2386=
    -->
    <!-- Allow Incoming SIP Messages from SIP Proxy Only. 0 - No, 1 - Yes -->
    <P243>1</P243>
    <!-- Use Privacy Header. 0 - Default, 1 - No, 2 - Yes -->
    <P2338>0</P2338>
    <!-- Use P-Preferred-Identity Header. 0 - Default, 1 - No, 2 - Yes -->
    <P2339>0</P2339>
    <!-- SIP T1 Timeout -->
    <P209>50</P209>
    <!-- SIP T2 Interval -->
    <P250>400</P250>
    <!-- SIP Timer D -->
    <P2387>0</P2387>
    <!-- DTMF Payload Type -->
    <P79>101</P79>
    <!-- Preferred DTMF method -->
    <!-- 100 - In-audio, 101 - RFC2833, 102 - SIP INFO -->
    <!-- Priority 1 -->
    <P850>101</P850>
    <!-- Priority 2 -->
    <P851>102</P851>
    <!-- Priority 3 -->
    <P852>100</P852>
    <!-- Enable Call Features. 0 - No, 1 - Yes -->
    <P191>0</P191>
    <!-- Dial Plan -->
    <P4200>{ x+ | \+x+ | *x+ | *xx*x+ }</P4200>
    <!-- Send Anonymous. 0 - No, 1 - Yes -->
    <P65>0</P65>
    <!-- Session Expiration -->
    <P260>90</P260>
    <!-- Minimum SE -->
    <P261>90</P261>

    <!--
      ################################################################################
      # Codec/Voice Quality settings                                                 #
      ################################################################################
